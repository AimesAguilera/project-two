{{!-- Employer Modal --}}
<div class="modal fade" id="homeEmpModal" tabindex="-1" role="dialog" aria-labelledby="homeEmpLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="homeEmpLabel">Employers</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <form>
          <div class="form-group">
            <label for="bus-name">Company Name</label>
            <input type="text" class="form-control" id="bus-name">
            <label for="bus-url">Company Website</label>
            <input type="url" class="form-control" id="bus-url">
            <label for="bus-address">Company Address</label>
            <input type="text" class="form-control" id="bus-address">
            <label for="bus-bio">Company Bio</label>
            <input type="url" class="form-control" id="bus-bio">
            <label for="bus-photo">Company Photo</label>
            <input type="url" class="form-control" id="bus-photo">
            <label for="busSkillOne">Select Skill</label>
            <select class="form-control" id="busSkillOne">
              <option>HTML5</option>
              <option>CSS3</option>
              <option>JavaScript</option>
              <option>SQL</option>
              <option>React</option>
            </select>
            <label for="busSkillTwo">Select Skill</label>
            <select class="form-control" id="busSkillTwo">
              <option>HTML5</option>
              <option>CSS3</option>
              <option>JavaScript</option>
              <option>SQL</option>
              <option>React</option>
            </select>
            <label for="busSkillThree">Select Skill</label>
            <select class="form-control" id="busSkillThree">
              <option>HTML5</option>
              <option>CSS3</option>
              <option>JavaScript</option>
              <option>SQL</option>
              <option>React</option>
            </select>
          </div>
        </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <a href="/employers" class="btn btn-primary" id="busSubmit">Submit</a>
      </div>
    </div>
  </div>
</div>

