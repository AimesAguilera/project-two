{{!-- Aimes --}}
<tr>
  <td>{{dataValues.name}}</td>
  <td>{{dataValues.skillOne}}, {{dataValues.skillTwo}}, {{dataValues.skillThree}}</td>
  <td><button type='button' class='businessButton btn btn-primary' data-id="{{dataValues.id}}">
      expand
    </button></td>
</tr>

<div class="modal" id="exampleModal-{{id}}" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
  aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">{{dataValues.name}}</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <h5>Top skills for this position:</h5>
        <p>{{dataValues.skillOne}}, {{dataValues.skillTwo}}, {{dataValues.skillThree}}</p>
        <hr>
        <h5>Job Description:</h5>
        <p>{{dataValues.jobDescription}}</p>
        <hr>
        <a href={{dataValues.website}} class="btn btn-primary" target="_blank"
          rel="noopener noreferrer">{{dataValues.name}} Website</a>
        <a href={{dataValues.jobPosting}} class="btn btn-primary" target="_blank" rel="noopener noreferrer">Job
          Posting</a>
        <hr>
        <div class="form-group form-check" id="interestedForm">
          <input class="form-check-input" type="checkbox" value="true" id="interestCheckbox">
          <label class="form-check-label" for="interestedCheckbox">
            Interested? Check the box & hit Send to let {{dataValues.name}}'s recruiter know!
          </label>
          <button type="button" id='sendInterest' class="btn btn-primary" data-name='{{dataValues.name}}'>Send</button>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>